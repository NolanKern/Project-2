<div class="container">
    <div class="jumbotron">
        <h1>SKELETONNE</h1>
    </div>

    <div class="form-group">
        <label for="userEmail">Email address</label>
        <input type="email" class="form-control" id="userEmail" aria-describedby="emailHelp" placeholder="Enter email">
        <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
    </div>
    <div class="form-group">
        <label for="userPswd">Password</label>
        <input type="password" class="form-control" id="userPswd" placeholder="Password">
    </div>

    <button id="btnLogin" class="btn btn-primary">Log In</button>
    <button id="btnSignUp" class="btn btn-secondary">Sign Up</button>
    <button id="btnLogout" class="btn btn-action">Log Out</button>


</div>